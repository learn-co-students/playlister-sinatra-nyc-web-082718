<h1>Enter the information for a new song:</h1><br><br><br>

<form class="" action="/songs" method="post">
  <input type="text" id="Name" name="song[name]" placeholder="song name"><br><br>
  <input type="text" id="Artist Name" name="song[artist][name]" placeholder="artist name"><br><br>
  <% @genres.each do |genre| %>
  <%= genre.name %> <input id="<%=genre.name%>" type="checkbox" name="genres[]" value="<%=genre.id%>"><br>
  <% end %>
  <input type="submit" id="submit" name="" value="Create">
</form>
